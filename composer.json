{
    "name": "krak/php-inc",
    "description": "Php inc file generator",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "RJ Garcia",
            "email": "rj@bighead.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Krak\\PhpInc\\": "src"
        },
        "files": ["src/autoload.php"]
    },
    "require": {
        "composer-plugin-api": "^1.1 || ^2.0",
        "php": "8.0.*"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "peridot-php/leo": "^1.6",
        "peridot-php/peridot": "^1.19"
    },
    "extra": {
        "class": "Krak\\PhpInc\\PhpIncPlugin"
    }
}
